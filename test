local P=game:GetService("Players")
local RS=game:GetService("ReplicatedStorage")
local HS=game:GetService("HttpService")
local p=P.LocalPlayer
pcall(function()HS.HttpEnabled=true end)
wait(1)
local g=Instance.new("ScreenGui")
g.Name="BubblegumScript"
g.Parent=p.PlayerGui
local toggleBtn=Instance.new("TextButton")
toggleBtn.Name="ToggleButton"
toggleBtn.Size=UDim2.new(0,40,0,40)
toggleBtn.Position=UDim2.new(1,-45,0,5)
toggleBtn.BackgroundColor3=Color3.fromRGB(60,120,200)
toggleBtn.BorderSizePixel=0
toggleBtn.Text="GUI"
toggleBtn.TextColor3=Color3.fromRGB(255,255,255)
toggleBtn.TextSize=12
toggleBtn.Font=Enum.Font.GothamBold
toggleBtn.Parent=g
local toggleCorner=Instance.new("UICorner")
toggleCorner.CornerRadius=UDim.new(0,6)
toggleCorner.Parent=toggleBtn
local f=Instance.new("Frame")
f.Size=UDim2.new(0,320,0,280)
f.Position=UDim2.new(0.5,-160,0.5,-140)
f.BackgroundColor3=Color3.fromRGB(30,30,30)
f.BorderSizePixel=0
f.Parent=g
local t=Instance.new("TextLabel")
t.Size=UDim2.new(1,0,0,40)
t.BackgroundColor3=Color3.fromRGB(40,40,40)
t.Text="Bubblegum Script"
t.TextColor3=Color3.fromRGB(255,255,255)
t.TextSize=18
t.Font=Enum.Font.GothamBold
t.Parent=f
local b1=Instance.new("TextButton")
b1.Size=UDim2.new(0,280,0,35)
b1.Position=UDim2.new(0.5,-140,0,50)
b1.BackgroundColor3=Color3.fromRGB(60,60,60)
b1.Text="Auto Hatch: OFF"
b1.TextColor3=Color3.fromRGB(255,255,255)
b1.TextSize=14
b1.Font=Enum.Font.Gotham
b1.Parent=f
local webhookLabel=Instance.new("TextLabel")
webhookLabel.Size=UDim2.new(0,280,0,20)
webhookLabel.Position=UDim2.new(0.5,-140,0,95)
webhookLabel.BackgroundTransparency=1
webhookLabel.Text="Webhook URL:"
webhookLabel.TextColor3=Color3.fromRGB(200,200,200)
webhookLabel.TextSize=10
webhookLabel.Font=Enum.Font.Gotham
webhookLabel.TextXAlignment=Enum.TextXAlignment.Left
webhookLabel.Parent=f
local webhookInput=Instance.new("TextBox")
webhookInput.Size=UDim2.new(0,280,0,30)
webhookInput.Position=UDim2.new(0.5,-140,0,115)
webhookInput.BackgroundColor3=Color3.fromRGB(40,40,40)
webhookInput.BorderSizePixel=0
webhookInput.Text=""
webhookInput.PlaceholderText="Enter Discord webhook URL"
webhookInput.TextColor3=Color3.fromRGB(255,255,255)
webhookInput.TextSize=10
webhookInput.Font=Enum.Font.Gotham
webhookInput.Parent=f
local secretLabel=Instance.new("TextLabel")
secretLabel.Size=UDim2.new(0,280,0,20)
secretLabel.Position=UDim2.new(0.5,-140,0,135)
secretLabel.BackgroundTransparency=1
secretLabel.Text="Secret Pets (comma separated):"
secretLabel.TextColor3=Color3.fromRGB(200,200,200)
secretLabel.TextSize=10
secretLabel.Font=Enum.Font.Gotham
secretLabel.TextXAlignment=Enum.TextXAlignment.Left
secretLabel.Parent=f
local secretInput=Instance.new("TextBox")
secretInput.Size=UDim2.new(0,280,0,30)
secretInput.Position=UDim2.new(0.5,-140,0,175)
secretInput.BackgroundColor3=Color3.fromRGB(40,40,40)
secretInput.BorderSizePixel=0
secretInput.Text="snowball"
secretInput.TextColor3=Color3.fromRGB(255,255,255)
secretInput.TextSize=10
secretInput.Font=Enum.Font.Gotham
secretInput.Parent=f
local s=Instance.new("TextLabel")
s.Size=UDim2.new(0,280,0,50)
s.Position=UDim2.new(0.5,-140,0,215)
s.BackgroundTransparency=1
s.Text="Status: Ready"
s.TextColor3=Color3.fromRGB(200,200,200)
s.TextSize=10
s.Font=Enum.Font.Gotham
s.TextWrapped=true
s.Parent=f
local en=false
local WEBHOOK=""
local secretPets={"snowball"}
local function updateSecretPets()
local text=secretInput.Text
if text and text~=""then
secretPets={}
for pet in string.gmatch(text,"([^,]+)")do
table.insert(secretPets,string.lower(string.gsub(pet,"^%s*(.-)%s*$","%1")))
end
end
end
secretInput.FocusLost:Connect(function()
updateSecretPets()
end)
local function sendWebhook(msg)
if WEBHOOK==""then return end
pcall(function()
HS:PostAsync(WEBHOOK,HS:JSONEncode({content=msg}),Enum.HttpContentType.ApplicationJson)
s.Text="Webhook sent!"
task.wait(1)
s.Text="Status: Ready"
end)
end
local function sendNotification(msg)
game:GetService("StarterGui"):SetCore("SendNotification",{Title="Secret Pet!",Text=msg,Duration=10})
sendWebhook(msg)
end
local hooked={}
local function hookRemote(r)
if hooked[r]then return end
hooked[r]=true
if r:IsA("RemoteEvent")then
r.OnClientEvent:Connect(function(...)
local args={...}
if args[2]=="ServerHatch"and type(args[1])=="string"then
local message=args[1]
local playerName=p.Name
local nameMatch=string.match(message,"<font color=\"#[^\"]+\">([^<]+)</font> just hatched")
if nameMatch and nameMatch==playerName then
local petMatch=string.match(message,"just hatched a <font color=\"#[^\"]+\">([^<]+)</font>")
if petMatch then
petMatch=string.gsub(petMatch,"^%s*(.-)%s*$","%1")
local petLower=string.lower(petMatch)
for _,secret in ipairs(secretPets)do
if string.find(petLower,secret)then
local msg=playerName.." hatched a "..petMatch.."!"
s.Text="SECRET: "..petMatch
sendNotification(msg)
break
end
end
end
end
end
end)
end
end
spawn(function()
wait(2)
for _,v in pairs(RS:GetDescendants())do
if v:IsA("RemoteEvent")then hookRemote(v)end
end
end)
b1.Activated:Connect(function()
en=not en
if en then
b1.Text="Auto Hatch: ON"
b1.BackgroundColor3=Color3.fromRGB(50,200,50)
s.Text="Auto Hatch: Active"
spawn(function()
local VIM=game:GetService("VirtualInputManager")
while en do
VIM:SendKeyEvent(true,Enum.KeyCode.E,false,game)
task.wait(0.05)
VIM:SendKeyEvent(false,Enum.KeyCode.E,false,game)
task.wait(0.2)
end
end)
else
b1.Text="Auto Hatch: OFF"
b1.BackgroundColor3=Color3.fromRGB(60,60,60)
s.Text="Auto Hatch: Stopped"
end
end)
webhookInput.FocusLost:Connect(function()
WEBHOOK=webhookInput.Text
if WEBHOOK~=""then
s.Text="Webhook URL saved!"
task.wait(1)
s.Text="Status: Ready"
end
end)
toggleBtn.Activated:Connect(function()
f.Visible=not f.Visible
if f.Visible then
toggleBtn.Text="GUI"
toggleBtn.BackgroundColor3=Color3.fromRGB(60,120,200)
else
toggleBtn.Text=">>>"
toggleBtn.BackgroundColor3=Color3.fromRGB(100,100,100)
end
end)
game:GetService("StarterGui"):SetCore("SendNotification",{Title="Loaded",Text="Ready!",Duration=3})
