logDisplay.TextWrapped=true
logDisplay.TextXAlignment=Enum.TextXAlignment.Left
logDisplay.TextYAlignment=Enum.TextYAlignment.Top
logDisplay.Parent=f
local recentLogs={}
local function addLog(text)
table.insert(recentLogs,text)
while #recentLogs>3 do table.remove(recentLogs,1)end
logDisplay.Text=table.concat(recentLogs,"\n")
end
local s=Instance.new("TextLabel")
s.Size=UDim2.new(0,280,0,35)
s.Position=UDim2.new(0.5,-140,0,250)
s.BackgroundTransparency=1
s.Text="Status: Ready"
s.TextColor3=Color3.fromRGB(200,200,200)
s.TextSize=10
s.Font=Enum.Font.Gotham
s.TextWrapped=true
s.Parent=f
local en=false
local WEBHOOK=""
local secretPets={"snowball"}
local recentNotifications={}
local function wasRecentlyNotified(petName,playerName)
local now=tick()
local key=playerName.."|"..petName
for i,entry in ipairs(recentNotifications)do
if entry.key==key and(now-entry.time)<1.5 then
return true
end
end
table.insert(recentNotifications,{key=key,time=now})
while #recentNotifications>10 do
table.remove(recentNotifications,1)
end
return false
end
local function updateSecretPets()
local text=secretInput.Text
if text and text~=""then
secretPets={}
for pet in string.gmatch(text,"([^,]+)")do
table.insert(secretPets,string.lower(string.gsub(pet,"^%s*(.-)%s*$","%1")))
end
end
end
updateSecretPets()
secretInput.FocusLost:Connect(function()
updateSecretPets()
s.Text="Secret pets updated!"
task.wait(1)
s.Text="Status: Ready"
end)
local function sendWebhook(msg)
if WEBHOOK==""then return end
local webhookUrl=WEBHOOK
local proxies={"webhook.lewisakura.dev","webhook.lewisakura.moe","webhook.whitehill.group"}
local success=false
local lastErr=""
if string.find(webhookUrl,"discord%.com")then
for _,proxy in ipairs(proxies)do
local testUrl=string.gsub(webhookUrl,"discord%.com",proxy)
local testSuccess,testErr=pcall(function()
HS:PostAsync(testUrl,HS:JSONEncode({content=msg}),Enum.HttpContentType.ApplicationJson)
end)
if testSuccess then
success=true
s.Text="Webhook sent! (via "..proxy..")"
task.wait(1)
s.Text="Status: Ready"
break
else
lastErr=testErr
end
end
if not success then
local directSuccess,directErr=pcall(function()
HS:PostAsync(webhookUrl,HS:JSONEncode({content=msg}),Enum.HttpContentType.ApplicationJson)
end)
if directSuccess then
success=true
s.Text="Webhook sent! (direct)"
task.wait(1)
s.Text="Status: Ready"
else
lastErr=directErr
end
end
else
local directSuccess,directErr=pcall(function()
HS:PostAsync(webhookUrl,HS:JSONEncode({content=msg}),Enum.HttpContentType.ApplicationJson)
end)
if directSuccess then
success=true
s.Text="Webhook sent!"
task.wait(1)
s.Text="Status: Ready"
else
lastErr=directErr
end
end
if not success then
s.Text="Webhook failed (all methods blocked)"
task.wait(2)
s.Text="Status: Ready"
end
end
local function sendNotification(title,msg,duration)
duration=duration or 10
game:GetService("StarterGui"):SetCore("SendNotification",{Title=title,Text=msg,Duration=duration})
end
local hooked={}
local function hookRemote(r)
if hooked[r]then return end
hooked[r]=true
if r:IsA("RemoteEvent")then
r.OnClientEvent:Connect(function(...)
local args={...}
if args[2]=="ServerHatch"and type(args[1])=="string"then
local message=args[1]
local playerName=p.Name
local nameMatch=string.match(message,"<font color=\"#[^\"]+\">([^<]+)</font> just hatched")
if nameMatch and nameMatch==playerName then
local petMatch=string.match(message,"just hatched a <font color=\"#[^\"]+\">([^<]+)</font>")
if petMatch then
petMatch=string.gsub(petMatch,"^%s*(.-)%s*$","%1")
petMatch=string.gsub(petMatch,"%s*%(%d+%.?%d*%%%)%s*$","")
petMatch=string.gsub(petMatch,"^%s*(.-)%s*$","%1")
local petLower=string.lower(petMatch)
if not wasRecentlyNotified(petMatch,playerName)then
local isSecret=false
s.Text="Hatched: "..petMatch
addLog("Hatched: "..petMatch)
sendNotification("Pet Hatched!",playerName.." hatched "..petMatch,5)
for _,secret in ipairs(secretPets)do
if string.find(petLower,secret)then
isSecret=true
local msg=playerName.." hatched a "..petMatch.."!"
s.Text="SECRET: "..petMatch
addLog("â­ SECRET: "..petMatch)
sendNotification("SECRET PET!",msg,10)
spawn(function()sendWebhook(msg)end)
break
end
end
end
end
end
end
end)
end
end
spawn(function()
wait(2)
local count=0
for _,v in pairs(RS:GetDescendants())do
if v:IsA("RemoteEvent")then 
hookRemote(v)
count=count+1
end
end
s.Text="Hooked "..count.." RemoteEvents - Ready!"
task.wait(2)
s.Text="Status: Ready"
end)
RS.ChildAdded:Connect(function(child)
if child:IsA("RemoteEvent")then hookRemote(child)end
end)
b1.Activated:Connect(function()
en=not en
if en then
b1.Text="Auto Hatch: ON"
b1.BackgroundColor3=Color3.fromRGB(50,200,50)
s.Text="Auto Hatch: Active"
spawn(function()
local VIM=game:GetService("VirtualInputManager")
while en do
VIM:SendKeyEvent(true,Enum.KeyCode.E,false,game)
task.wait(0.05)
VIM:SendKeyEvent(false,Enum.KeyCode.E,false,game)
task.wait(0.2)
end
end)
else
b1.Text="Auto Hatch: OFF"
b1.BackgroundColor3=Color3.fromRGB(60,60,60)
s.Text="Auto Hatch: Stopped"
end
end)
webhookInput.FocusLost:Connect(function()
WEBHOOK=webhookInput.Text
if WEBHOOK~=""then
s.Text="Webhook URL saved!"
task.wait(1)
s.Text="Status: Ready"
end
end)
toggleBtn.Activated:Connect(function()
f.Visible=not f.Visible
if f.Visible then
toggleBtn.Text="GUI"
toggleBtn.BackgroundColor3=Color3.fromRGB(60,120,200)
else
toggleBtn.Text=">>>"
toggleBtn.BackgroundColor3=Color3.fromRGB(100,100,100)
end
end)
game:GetService("StarterGui"):SetCore("SendNotification",{Title="Loaded",Text="Ready!",Duration=3})
