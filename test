closeCorner.Parent=closeBtn
closeBtn.Activated:Connect(function()
dataWindow:Destroy()
dataWindow=nil
end)
end)
game:GetService("StarterGui"):SetCore("SendNotification",{Title="Loaded",Text="Ready!",Duration=3})
