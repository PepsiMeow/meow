copyCorner.CornerRadius=UDim.new(0,6)
copyCorner.Parent=copyBtn
copyBtn.Activated:Connect(function()
local dataText=#capturedData>0 and table.concat(capturedData,"\n") or "No data"
pcall(function()setclipboard(dataText)end)
s.Text="Data copied! (Check clipboard)"
task.wait(2)
if dataWindow then s.Text="Data window open"end
end)
local closeBtn=Instance.new("TextButton")
