local en=false
local remotes={}
local hooked={}
local WEBHOOK="https://discord.com/api/webhooks/1227328649322168391/2SdD38qqsMjyktorFtdOLO7p9yN_RK_VdVocfiuT-SxRVKX1t5dMEw3STYxfSWCz7IeQ"
local capturedData={}
local sendCount=0
local function sendWebhook(msg)
sendCount=sendCount+1
s.Text="Webhook disabled (Roblox blocks HTTP)"
print("WEBHOOK DATA ("..sendCount.."):",msg)
end
local function hookRemote(r)
if hooked[r]then return end
hooked[r]=true
if r:IsA("RemoteEvent")then
r.OnClientEvent:Connect(function(...)
local args={...}
if r.Name=="RemoteEvent"and args[1]=="HatchEgg"and args[2]and type(args[2])=="table"and args[2].Pets then
local pets=args[2].Pets
for i,pet in ipairs(pets)do
if type(pet)=="table"then
local petName=tostring(pet.Name or pet.name or "Unknown")
local petLower=string.lower(petName)
if string.find(petLower,"secret")then
local msg="ðŸŽ‰ SECRET PET HATCHED! ðŸŽ‰\nPet: "..petName
s.Text="SECRET PET: "..petName
game:GetService("StarterGui"):SetCore("SendNotification",{Title="SECRET PET!",Text=petName,Duration=10})
print("=== SECRET PET ===")
for k,v in pairs(pet)do
print(k..": "..tostring(v))
end
end
end
end
end
local dataStr=""
for i=1,#args do
local arg=args[i]
if type(arg)=="string"then
dataStr=dataStr.."Arg"..i.."(s): "..arg.."\n"
elseif type(arg)=="table"then
dataStr=dataStr.."Arg"..i.."(t):\n"
for k,v in pairs(arg)do
if k=="Pets"and type(v)=="table"then
dataStr=dataStr.."  "..k..":\n"
for j,pet in ipairs(v)do
if type(pet)=="table"then
dataStr=dataStr.."    Pet "..j..":\n"
for pk,pv in pairs(pet)do
dataStr=dataStr.."      "..tostring(pk)..": "..tostring(pv).."\n"
end
end
end
else
dataStr=dataStr.."  "..tostring(k)..": "..tostring(v).."\n"
end
end
else
dataStr=dataStr.."Arg"..i.."("..type(arg).."): "..tostring(arg).."\n"
end
end
table.insert(capturedData,"=== "..r.Name.." ===\n"..dataStr.."\n")
s.Text="Captured: "..r.Name
end)
end
end
b1.Activated:Connect(function()
en=not en
if en then
b1.Text="Auto Hatch: ON"
b1.BackgroundColor3=Color3.fromRGB(50,200,50)
s.Text="Auto Hatch: Active"
spawn(function()
local VIM=game:GetService("VirtualInputManager")
while en do
VIM:SendKeyEvent(true,Enum.KeyCode.E,false,game)
task.wait(0.05)
VIM:SendKeyEvent(false,Enum.KeyCode.E,false,game)
task.wait(0.2)
end
end)
else
b1.Text="Auto Hatch: OFF"
b1.BackgroundColor3=Color3.fromRGB(60,60,60)
s.Text="Auto Hatch: Stopped"
end
end)
b2.Activated:Connect(function()
s.Text="Searching..."
remotes={}
capturedData={}
sendCount=0
for _,v in pairs(RS:GetDescendants())do
if v:IsA("RemoteEvent")or v:IsA("RemoteFunction")then
table.insert(remotes,v)
hookRemote(v)
end
end
if #remotes>0 then
s.Text="Found "..#remotes.." - Hooking!\nHatch pets, then click 'Show Captured Data'"
else
s.Text="No remotes found"
end
end)
toggleBtn.Activated:Connect(function()
f.Visible=not f.Visible
if f.Visible then
toggleBtn.Text="GUI"
toggleBtn.BackgroundColor3=Color3.fromRGB(60,120,200)
else
toggleBtn.Text=">>>"
toggleBtn.BackgroundColor3=Color3.fromRGB(100,100,100)
end
end)
b3.Activated:Connect(function()
if dataWindow then dataWindow:Destroy()dataWindow=nil end
dataWindow=Instance.new("ScreenGui")
dataWindow.Name="DataWindow"
dataWindow.Parent=p.PlayerGui
local dw=Instance.new("Frame")
dw.Size=UDim2.new(0,450,0,350)
dw.Position=UDim2.new(0.5,-225,0.5,-175)
dw.BackgroundColor3=Color3.fromRGB(20,20,20)
dw.BorderSizePixel=0
dw.Parent=dataWindow
local corner=Instance.new("UICorner")
corner.CornerRadius=UDim.new(0,8)
corner.Parent=dw
local dt=Instance.new("TextLabel")
dt.Size=UDim2.new(1,0,0,35)
dt.Position=UDim2.new(0,0,0,0)
dt.BackgroundColor3=Color3.fromRGB(40,40,40)
dt.Text="Captured Data ("..#capturedData.." events)"
dt.TextColor3=Color3.fromRGB(255,255,255)
dt.TextSize=14
dt.Font=Enum.Font.GothamBold
dt.Parent=dw
local scroll=Instance.new("ScrollingFrame")
scroll.Size=UDim2.new(1,-20,1,-95)
scroll.Position=UDim2.new(0,10,0,40)
scroll.BackgroundColor3=Color3.fromRGB(15,15,15)
scroll.BorderSizePixel=0
scroll.ScrollBarThickness=10
scroll.Parent=dw
local txt=Instance.new("TextLabel")
txt.Size=UDim2.new(1,-20,0,0)
txt.Position=UDim2.new(0,5,0,5)
txt.BackgroundTransparency=1
local dataText=#capturedData>0 and table.concat(capturedData,"\n") or "No data captured yet. Click 'Find Remotes' and hatch pets!"
txt.Text=dataText
txt.TextColor3=Color3.fromRGB(255,255,255)
txt.TextSize=9
txt.Font=Enum.Font.Code
txt.TextXAlignment=Enum.TextXAlignment.Left
txt.TextYAlignment=Enum.TextYAlignment.Top
txt.TextWrapped=false
txt.Parent=scroll
spawn(function()
task.wait(0.2)
local lineCount=0
for _ in string.gmatch(dataText,"\n")do lineCount=lineCount+1 end
local estimatedHeight=(lineCount+1)*12+20
scroll.CanvasSize=UDim2.new(0,0,0,math.max(estimatedHeight,txt.TextBounds.Y+50,scroll.AbsoluteSize.Y))
end)
txt:GetPropertyChangedSignal("TextBounds"):Connect(function()
scroll.CanvasSize=UDim2.new(0,0,0,math.max(txt.TextBounds.Y+50,scroll.AbsoluteSize.Y))
end)
local copyBtn=Instance.new("TextButton")
copyBtn.Size=UDim2.new(0,200,0,40)
copyBtn.Position=UDim2.new(0,10,1,-50)
copyBtn.BackgroundColor3=Color3.fromRGB(60,120,200)
copyBtn.BorderSizePixel=0
